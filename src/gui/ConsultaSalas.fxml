<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="258.0" prefWidth="435.0"
	xmlns="http://javafx.com/javafx/15.0.1"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="gui.ConsultaSalasController">
	<children>
		<Label text="Consulta Sala" wrapText="true">
			<font>
				<Font name="System Bold" size="12.0" />
			</font>
			<padding>
				<Insets bottom="10.0" left="20.0" top="10.0" />
			</padding>
		</Label>
		<Label text="Digite o nome da sala a consultar" />
		<TextField fx:id="txtNomeSala"
			onAction="#onTxtNomeSalaAction" promptText="Nome da sala" />
		<Button fx:id="btConsultaSala" mnemonicParsing="false"
			onAction="#onBtConsultaSalaAction" text="Consultar">
			<VBox.margin>
				<Insets top="10.0" />
			</VBox.margin>
		</Button>
	</children>
</VBox>
